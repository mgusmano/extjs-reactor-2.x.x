{
  "name": "@extjs/react-classic-boilerplate",
  "private": true,
  "version": "1.1.1",
  "description": "A boilerplate project showing how to integrate React and Ext JS using the classic toolkit.",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf build",
    "start": "webpack-dev-server --progress --colors",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --env.prod=true --progress --colors",
    "prod": "cd build && static-server --port 8080"
  },
  "author": "Sencha, Inc.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sencha/extjs-reactor.git"
  },
  "dependencies": {
    "@extjs/reactor": "^1.1.1",
    "babel-polyfill": "^6.23.0",
    "babel-runtime": "^6.23.0",
    "react": "^15.4.0",
    "react-dom": "^15.4.0"
  },
  "devDependencies": {
    "@extjs/reactor-babel-plugin": "^1.1.1",
    "@extjs/reactor-webpack-plugin": "^1.1.1",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.13.0",
    "cross-env": "^5.0.0",
    "html-webpack-plugin": "^2.24.1",
    "open-browser-webpack-plugin": "0.0.5",
    "portfinder": "^1.0.13",
    "react-hot-loader": "^3.0.0-beta.7",
    "rimraf": "^2.6.1",
    "static-server": "^2.0.5",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  }
}
